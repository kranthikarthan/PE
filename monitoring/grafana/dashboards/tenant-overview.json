{
  "dashboard": {
    "id": null,
    "title": "Tenant Overview Dashboard",
    "tags": ["payment-engine", "tenant", "multi-tenant"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "tenant_id",
          "type": "query",
          "query": "label_values(payment_transactions_total, tenant_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "options": [],
          "current": {
            "value": "$__all",
            "text": "All"
          }
        },
        {
          "name": "time_range",
          "type": "interval",
          "query": "1m,5m,10m,30m,1h,6h,12h,1d,7d,14d,30d",
          "refresh": 2,
          "current": {
            "value": "5m",
            "text": "5m"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Tenant Transaction Volume",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(payment_transactions_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 2,
        "title": "Tenant Error Rates",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "(sum(rate(payment_transactions_total{tenant_id=~\"$tenant_id\",status=\"ERROR\"}[$time_range])) by (tenant_id) / sum(rate(payment_transactions_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id)) * 100",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Tenant Resource Usage",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "tenant_resource_usage_percentage{tenant_id=~\"$tenant_id\"}",
            "legendFormat": "{{tenant_id}} - {{resource_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Active Tenants",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "count(count by (tenant_id) (payment_transactions_total{tenant_id=~\"$tenant_id\"}))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "Transaction Volume by Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(payment_transactions_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right"}
        }
      },
      {
        "id": 6,
        "title": "Error Rate by Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "(sum(rate(payment_transactions_total{tenant_id=~\"$tenant_id\",status=\"ERROR\"}[$time_range])) by (tenant_id) / sum(rate(payment_transactions_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id)) * 100",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 7,
        "title": "API Requests by Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(api_requests_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id, method)",
            "legendFormat": "{{tenant_id}} - {{method}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "title": "Rate Limiting by Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(api_requests_rate_limited_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 9,
        "title": "ISO 20022 Messages by Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(iso20022_messages_processed_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id, message_type)",
            "legendFormat": "{{tenant_id}} - {{message_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 10,
        "title": "Configuration Changes by Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(tenant_configuration_changes_total{tenant_id=~\"$tenant_id\"}[$time_range])) by (tenant_id, config_type)",
            "legendFormat": "{{tenant_id}} - {{config_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 11,
        "title": "Feature Flag Rollouts",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "tenant_feature_flag_rollout_percentage{tenant_id=~\"$tenant_id\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background",
              "align": "center"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "rollout_percentage"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "color", "value": {"mode": "thresholds"}},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 25},
                    {"color": "green", "value": 100}
                  ]
                }}
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true, "job": true, "instance": true},
              "renameByName": {
                "tenant_id": "Tenant ID",
                "feature_name": "Feature",
                "Value": "Rollout %"
              }
            }
          }
        ]
      },
      {
        "id": 12,
        "title": "Tenant Resource Limits",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "tenant_resource_usage{tenant_id=~\"$tenant_id\"}",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "tenant_resource_limit{tenant_id=~\"$tenant_id\"}",
            "format": "table",
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true, "job": true, "instance": true},
              "renameByName": {
                "tenant_id": "Tenant ID",
                "resource_type": "Resource Type",
                "Value #A": "Current Usage",
                "Value #B": "Limit"
              }
            }
          },
          {
            "id": "calculateField",
            "options": {
              "alias": "Usage %",
              "binary": {
                "left": "Current Usage",
                "operator": "/",
                "right": "Limit"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Usage %"},
              "properties": [
                {"id": "unit", "value": "percentunit"},
                {"id": "color", "value": {"mode": "thresholds"}},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 0.7},
                    {"color": "red", "value": 0.9}
                  ]
                }}
              ]
            }
          ]
        }
      }
    ]
  }
}